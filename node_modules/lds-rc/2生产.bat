@echo off
%安装模块%
if not exist "node_modules" (
	call npm install
)
%覆盖模块配置%
xcopy "node_modules_rewrite" "node_modules" /yeh
%构建生成%
call npm run build
%清理打包文件%
del /F /S /Q pack\*.zip
%构建打包文件%
for /D %%i in (pack\component\*) do winrar a -afzip -r -ep1 "%%i.zip" "%%i"

pause

@echo on